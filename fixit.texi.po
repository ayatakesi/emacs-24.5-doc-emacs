#: fixit.texi.orig:347
#, no-wrap
msgid "q"
msgstr ""

#. type: table
#: fixit.texi.orig:349
msgid "Quit interactive spell checking and kill the spell-checker subprocess."
msgstr ""

#. type: item
#: fixit.texi.orig:350
#, no-wrap
msgid "?"
msgstr ""

#. type: table
