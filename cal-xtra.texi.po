#: original_texis/cal-xtra.texi:587 original_texis/cal-xtra.texi:602
#, no-wrap
msgid "diary-islamic-insert-entry"
msgstr "diary-islamic-insert-entry"

#. type: item
#: original_texis/cal-xtra.texi:587
#, no-wrap
msgid "i i m"
msgstr "i i m"

#. type: findex
#: original_texis/cal-xtra.texi:589 original_texis/cal-xtra.texi:603
#, no-wrap
msgid "diary-islamic-insert-monthly-entry"
msgstr "diary-islamic-insert-monthly-entry"

#. type: item
#: original_texis/cal-xtra.texi:589
