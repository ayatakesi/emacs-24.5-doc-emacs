#: maintaining.texi.orig:1309 maintaining.texi.orig:1350
#, no-wrap
msgid "VC Pull"
msgstr ""

#. type: menuentry
#: maintaining.texi.orig:1309
msgid "Updating the contents of a branch."
msgstr ""

#. type: node
#: maintaining.texi.orig:1309 maintaining.texi.orig:1389
#, no-wrap
msgid "Merging"
msgstr ""

#. type: menuentry
#: maintaining.texi.orig:1309
msgid "Transferring changes between branches."
msgstr ""

#. type: node
#: maintaining.texi.orig:1309 maintaining.texi.orig:1436
#, no-wrap
msgid "Creating Branches"
msgstr ""

