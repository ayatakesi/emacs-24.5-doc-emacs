msgid "Commands to control what is visible."
msgstr ""

#. type: node
#: text.texi.orig:911 text.texi.orig:1140
#, no-wrap
msgid "Outline Views"
msgstr ""

#. type: menuentry
#: text.texi.orig:911
msgid "Outlines and multiple views."
msgstr ""

#. type: node
#: text.texi.orig:911 text.texi.orig:1160
#, no-wrap
