msgstr ""

#. type: item
#: calendar.texi.orig:984
#, no-wrap
msgid "M-x diary"
msgstr ""

#. type: table
#: calendar.texi.orig:986
msgid "Display all diary entries for today's date."
msgstr ""

#. type: item
#: calendar.texi.orig:986
#, no-wrap
